    "customStyles": {
        "styles": "/* Eliminar fondos de widgets para que se vea solo el background de la barra */ .simple-bar .widget { background: transparent !important; } .simple-bar .process { background: transparent !important; } .simple-bar .weather { background: transparent !important; } .simple-bar .netstats { background: transparent !important; } .simple-bar .cpu { background: transparent !important; } .simple-bar .memory { background: transparent !important; } .simple-bar .sound { background: transparent !important; } /* Estilos para workspaces - sin fondo por defecto */ .simple-bar .spaces .space { background: transparent !important; margin: 0 2px !important; padding: 2px !important; border-bottom: 2px solid transparent !important; } /* Solo workspaces activos tienen borde inferior blanco */ .simple-bar .spaces .space.focused, .simple-bar .spaces .space.visible { border-bottom: 2px solid #FFFFFF !important; }"
    }
